{
  "name": "luware-cache-host",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "npm version patch",
    "package:build": "ng-packagr -p projects/cache/ng-package.json",
    "package:test": "ng test cache",
    "package:lint": "ng lint cache",
    "package:lint-fix": "ng lint cache --fix=true",
    "package:version": "yarn --cwd projects\\cache version",
    "package:postVersion": "git commit projects\\cache\\package.json -m \"version ${npm_package_version}\" && git push",
    "package:publish": "yarn publish dist/luware-ui-cache --newVersion $npm_package_version --registry=https://base.luware.com/Internal/_packaging/Nimbus/npm/registry/",
    "package:debug:publish": "yarn package:build && node_modules/.bin/yalc publish dist/luware-ui-cache",
    "package:debug:update": "yarn package:build && node_modules/.bin/yalc push dist/luware-ui-cache"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "8.2.6",
    "@angular/core": "8.2.6",
    "moment": "2.24.0",
    "lodash": "4.17.15",
    "rxjs": "6.5.3",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular/platform-browser": "8.2.6",
    "@angular/platform-browser-dynamic": "8.2.6",
    "@angular-devkit/build-angular": "0.803.21",
    "@angular-devkit/build-ng-packagr": "0.803.21",
    "@angular/cli": "8.3.4",
    "@angular/compiler": "8.2.6",
    "@angular/compiler-cli": "8.2.6",
    "@angular/language-service": "8.2.6",
    "@types/jasmine": "3.4.6",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.22",
    "@types/uuid": "3.4.6",
    "codelyzer": "5.2.1",
    "core-js": "3.6.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-firefox-launcher": "1.2.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.5.1",
    "karma-phantomjs-launcher": "1.0.4",
    "ng-packagr": "5.7.1",
    "node-sass": "4.13.0",
    "protractor": "5.4.2",
    "puppeteer": "1.20.0",
    "ts-node": "8.4.1",
    "tsickle": "0.37.0",
    "tslib": "1.10.0",
    "tslint": "5.20.1",
    "typescript": "3.5.3",
    "yalc": "1.0.0-pre.35"
  }
}
